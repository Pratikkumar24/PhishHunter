{
    "manifest_version": 2,
    "name": "PhishHunter",
    "version": "1.0",
    "description": "A tool to detect phishing link",
    "icons": {
        "128": "images/icon128.png",
        "48": "images/icon48.png",
        "16": "images/icon16.png"
    },


    "background": {
        "scripts": ["templates/eventPage.js"],
        "persistant": false
    },


    "browser_action": {
        "default_icon": "images/icon128.png",
        "default_popup": "templates/popup.html"
    },

    "permissions": [
        "contextMenus",
        "storage",
        "notifications",
        "tabs"
    ]


}